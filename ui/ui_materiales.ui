  <property name="styleSheet">
   <string notr="true">/*
 * Archivo de Estilo QSS - Tema Oscuro (Dark Mode) - Estilo CRUD
 * Colores: Fondos Oscuros (#1E1E1E), Navegación (#383838), Énfasis Azul (#0078d7).
 */

/* === Estilo general === */
QMainWindow {
    /* Fondo principal oscuro */
    background-color: #1E1E1E;
    /* Color de texto claro */
    color: #F0F0F0;
    font-family: &quot;Segoe UI Variable&quot;, &quot;Roboto&quot;, &quot;Open Sans&quot;, sans-serif;
    font-size: 16px;
    font-weight: 300;
}

/* === Frames === */
QFrame {
    /* Fondo de frames de contenido oscuro (área principal) */
    background-color: #2D2D2D;
    border-radius: 10px;
    /* Borde sutil en modo oscuro */
    border: 1px solid #444444;
}

/* Barra superior */
#frame_superior {
    /* Color ligeramente más oscuro que el fondo de frames */
    background-color: #252525;
    border: none;
}

/* Panel lateral (Menú de navegación) */
#frame_lateral {
    /* Color para el panel lateral, más distinguible del fondo */
    background-color: #383838;
    border-right: 1px solid #444444; 
}

/* Área principal de contenido */
#frame_paginas {
    background-color: #2D2D2D;
}

/* === Botones de Barra Superior === */

/* Botón principal (Menú) - ESTILO ACTIVO */
#boton_menu {
    /* Azul claro primario */
    background-color: #0078d7;
    color: white;
    border: none;
    border-radius: 6px;
    padding: 8px 14px;
    font-weight: 400;
}
#boton_menu:hover {
    background-color: #006cbe;
}
#boton_menu:pressed {
    background-color: #005a9e;
}

/* Botón salir (Azul Activo) */
#boton_salir {
    background-color: #0078d7; 
    color: white;
    border: none;
    border-radius: 6px;
    padding: 8px 14px;
    font-weight: 400;
}
#boton_salir:hover {
    background-color: #006cbe;
}
#boton_salir:pressed {
    background-color: #005a9e;
}

/* === Botones de Navegación Lateral (CRUD) === */
QPushButton {
    /* Estilo base inactivo de los botones laterales */
    background-color: #383838; /* Fondo igual al panel lateral */
    border: none;
    color: #F0F0F0;
    font-weight: 400;
    font-size: 15px;
    text-align: left;
    padding: 10px 15px; /* Espaciado para el texto/icono */
}

/* Hover para botones laterales */
QPushButton:hover {
    /* Fondo gris ligeramente más claro al hacer hover */
    background-color: #4C4C4C; 
    color: #ffffff;
}

/* Botón lateral activo (como el botón &quot;Nuevo&quot;) */
#boton_nuevo { /* Si el botón de Nuevo tiene objectName=&quot;boton_nuevo&quot; */
    background-color: #0078d7; /* Azul primario */
    color: #ffffff;
}
#boton_nuevo:hover {
    background-color: #006cbe; /* Azul más oscuro al pasar el ratón */
}
#boton_nuevo:pressed {
    background-color: #005a9e;
}


/* === Botón de Acción Principal (Guardar) === */
#Guardar { /* Asumiendo que el botón &quot;Guardar&quot; tiene objectName=&quot;Guardar&quot; */
    background-color: #0078d7;
    color: white;
    border: none;
    border-radius: 6px;
    padding: 12px 20px; /* Mayor padding para un botón principal */
    font-weight: 500;
    font-size: 16px;
}

#Guardar:hover {
    background-color: #006cbe;
}

#Guardar:pressed {
    background-color: #005a9e;
}

/* === Campos de texto (QLineEdit) === */
QLineEdit {
    /* Fondo oscuro para la entrada de texto */
    background-color: #383838;
    color: #F0F0F0;
    border: 1px solid #555555;
    border-radius: 4px;
    padding: 6px 8px;
    font-size: 15px;
    font-weight: 300;
}

QLineEdit:focus {
    /* Borde de enfoque con el azul claro primario */
    border: 1px solid #0078d7;
    background-color: #3D4A5B; 
}

/* === Labels (ID empleado, Nombre, Cargo, Derechos reservados) === */
QLabel {
    /* Texto claro */
    color: #F0F0F0;
    font-weight: 300;
    font-size: 15px;
}

/* Label inferior o secundario (Derechos reservados 2025) */
#label { /* Si esta etiqueta tiene objectName=&quot;label&quot; */
    /* Texto gris claro para secundario */
    color: #A0A0A0;
    font-size: 13px;
    font-style: italic;
    padding-left: 15px; /* Para que coincida con la posición en la imagen */
}

/* === Scrollbars === */
QScrollBar:vertical {
    border: none;
    background: #383838;
    width: 10px;
    border-radius: 4px;
}
QScrollBar::handle:vertical {
    background: #555555;
    min-height: 20px;
    border-radius: 4px;
}
QScrollBar::handle:vertical:hover {
    background: #0078d7;
}
QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical {
    height: 0;
}</string>
