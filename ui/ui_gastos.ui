  <property name="styleSheet">
   <string notr="true">/*
 * Archivo de Estilo QSS - Tema Oscuro (Dark Mode) - Adaptado a la Imagen
 * Basado en la estructura de Tema Claro, pero con colores Oscuros.
 */

/* === Estilo general === */
QMainWindow {
    /* Fondo principal oscuro */
    background-color: #1E1E1E; /* Revertido a oscuro */
    /* Color de texto claro */
    color: #F0F0F0; /* Revertido a claro */
    font-family: &quot;Segoe UI Variable&quot;, &quot;Roboto&quot;, &quot;Open Sans&quot;, sans-serif;
    font-size: 16px;
    font-weight: 300;
}

/* === Frames === */
QFrame {
    /* Fondo de frames de contenido oscuro */
    background-color: #2D2D2D; /* Revertido a oscuro */
    border-radius: 10px;
    /* Borde sutil en modo oscuro */
    border: 1px solid #444444; /* Revertido a gris oscuro */
}

/* Barra superior */
#frame_superior {
    /* Color ligeramente más oscuro que el fondo de frames */
    background-color: #252525; /* Revertido a gris muy oscuro */
    border: none;
}

/* Panel lateral (Fondo del menú de navegación) */
#frame_lateral {
    /* Color para el panel lateral, más distinguible */
    background-color: #383838; /* Revertido a gris oscuro */
    border-right: 1px solid #444444; /* Revertido a gris oscuro */
}

/* Área principal */
#frame_paginas {
    background-color: #2D2D2D; /* Revertido a oscuro */
}

/* === Botones Genéricos (Fondo Oscuro/Gris Neutro) === */
QPushButton {
    /* Fondo de botón gris oscuro neutro (como los botones de las métricas) */
    background-color: #444444; 
    color: #F0F0F0;
    border: 1px solid #555555; 
    border-radius: 6px;
    padding: 8px 14px;
    font-weight: 400;
    font-size: 15px;
    letter-spacing: 0.3px;
}

QPushButton:hover {
    /* Azul claro original para énfasis */
    background-color: #0078d7;
    color: #ffffff;
    border: 1px solid #0078d7;
}

QPushButton:pressed {
    /* Azul más oscuro para estado presionado */
    background-color: #005a9e;
    color: #ffffff;
    border: 1px solid #005a9e;
}

/* Botón principal (Menú) - Azul Activo */
#boton_menu {
    background-color: #0078d7;
    color: white;
    border: none;
    font-weight: 400;
}
#boton_menu:hover {
    background-color: #006cbe;
}
#boton_menu:pressed {
    background-color: #005a9e;
}

/* Botón salir (Azul Activo) */
#boton_salir {
    background-color: #0078d7; /* Cambiado a azul para reflejar el estilo de la imagen */
    color: white;
    border: none;
}
#boton_salir:hover {
    background-color: #006cbe;
}
#boton_salir:pressed {
    background-color: #005a9e;
}

/* === Campos de texto (Para los rectángulos de las métricas) === */
QLineEdit {
    /* Fondo oscuro para la entrada de texto */
    background-color: #383838; /* Revertido a oscuro */
    color: #F0F0F0;
    border: 1px solid #555555; 
    border-radius: 4px;
    padding: 6px 8px;
    font-size: 15px;
    font-weight: 300;
}

QLineEdit:focus {
    /* Borde de enfoque con el azul claro primario */
    border: 1px solid #0078d7;
    background-color: #3D4A5B; /* Fondo sutil al enfocar */
}

/* === Labels === */
QLabel {
    /* Texto claro */
    color: #F0F0F0; /* Revertido a claro */
    font-weight: 300;
    font-size: 15px;
}

/* Label inferior o secundario */
#label {
    /* Texto gris claro para secundario (&quot;Derechos reservados 2025&quot;) */
    color: #A0A0A0; /* Revertido a gris claro */
    font-size: 13px;
    font-style: italic;
}

/* === Stacked Widget === */
QStackedWidget {
    background-color: #2D2D2D; /* Revertido a oscuro */
    border: 1px solid #444444;
    border-radius: 6px;
}

/* === Scrollbars === */
QScrollBar:vertical {
    border: none;
    background: #383838; /* Fondo oscuro */
    width: 10px;
    border-radius: 4px;
}
QScrollBar::handle:vertical {
    background: #555555; /* Handle gris oscuro */
    min-height: 20px;
    border-radius: 4px;
}
QScrollBar::handle:vertical:hover {
    background: #0078d7;
}
QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical {
    height: 0;
}

/* === Layouts === */
QVBoxLayout, QHBoxLayout {
    spacing: 10px;
}

/* === Botones laterales (Botones de Navegación) === */
/* Estilo base (inactivo en gris oscuro, como Gastos, Empleados, etc.) */
#boton_agregar, #boton_buscar, #boton_actualizar, #boton_eliminar, #boton_consultar, QWidget[objectName^=&quot;boton&quot;] {
    /* Fondo igual al panel lateral */
    background-color: #383838; 
    border: none; /* No tienen borde visible en la imagen */
    color: #F0F0F0;
    font-weight: 400;
    font-size: 15px;
    text-align: left; /* Alineación a la izquierda */
    padding-left: 15px; 
}

/* Estilo ACTIVO (Botón &quot;Proyectos&quot;) */
#Proyectos {
    background-color: #0078d7; /* Azul primario */
    color: #ffffff;
    border: none;
}

/* Estilo de hover para botones inactivos */
#boton_agregar:hover, 
#boton_buscar:hover, 
#boton_actualizar:hover, 
#boton_eliminar:hover,
#boton_consultar:hover,
QWidget[objectName^=&quot;boton&quot;]:hover {
    /* Gris ligeramente más claro */
    background-color: #4C4C4C; 
    color: #ffffff;
}

/* Asegurar que el botón activo mantenga su estilo al pasar el ratón o presionarlo */
#Proyectos:hover, 
#Proyectos:pressed {
    background-color: #006cbe;
    color: #ffffff;
}

/* === Tooltips === */
QToolTip {
    background-color: #383838;
    color: #F0F0F0;
    /* Borde con el azul claro primario */
    border: 1px solid #0078d7;
    padding: 6px;
    border-radius: 4px;
    font-size: 14px;
}</string>
