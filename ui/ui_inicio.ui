   <string notr="true">/*
 * Archivo de Estilo QSS - Tema Oscuro (Dark Mode) con Botones Azul Claro
 * Tonos: Oscuros para fondos, Azul claro (#0078d7) para énfasis.
 */

/* === Estilo general === */
QMainWindow {
    /* Fondo principal oscuro */
    background-color: #1E1E1E;
    /* Color de texto claro */
    color: #F0F0F0;
    font-family: &quot;Segoe UI Variable&quot;, &quot;Roboto&quot;, &quot;Open Sans&quot;, sans-serif;
    font-size: 16px;
    font-weight: 300;
}

/* === Frames === */
QFrame {
    /* Fondo de frames de contenido oscuro */
    background-color: #2D2D2D;
    border-radius: 10px;
    /* Borde sutil en modo oscuro */
    border: 1px solid #444444;
}

/* Barra superior */
#frame_superior {
    /* Color ligeramente más oscuro que el fondo de frames */
    background-color: #252525;
    border: none;
}

/* Panel lateral */
#frame_lateral {
    /* Color para el panel lateral, más distinguible */
    background-color: #383838;
    border-right: 1px solid #444444; 
}

/* Área principal */
#frame_paginas {
    background-color: #2D2D2D;
}

/* === Botones Genéricos (Fondo Claro) === */
QPushButton {
    /* Fondo de botón neutro */
    background-color: #444444;
    color: #F0F0F0;
    border: 1px solid #555555;
    border-radius: 6px;
    padding: 8px 14px;
    font-weight: 400;
    font-size: 15px;
    letter-spacing: 0.3px;
}

QPushButton:hover {
    /* Azul claro original para énfasis */
    background-color: #0078d7;
    color: #ffffff;
    border: 1px solid #0078d7;
}

QPushButton:pressed {
    /* Azul más oscuro para estado presionado */
    background-color: #005a9e;
    color: #ffffff;
    border: 1px solid #005a9e;
}

/* Botón principal (Menú) */
#boton_menu {
    /* Azul claro primario */
    background-color: #0078d7;
    color: white;
    border: none;
    font-weight: 400;
}
#boton_menu:hover {
    background-color: #006cbe;
}
#boton_menu:pressed {
    background-color: #005a9e;
}

/* Botón salir (Igual que boton_menu) */
#boton_salir {
    /* Azul claro primario */
    background-color: #0078d7;
    color: white;
    border: none;
    font-weight: 400;
}
#boton_salir:hover {
    background-color: #006cbe;
}
#boton_salir:pressed {
    background-color: #005a9e;
}

/* === Campos de texto === */
QLineEdit {
    /* Fondo oscuro para la entrada de texto */
    background-color: #383838;
    color: #F0F0F0;
    border: 1px solid #555555;
    border-radius: 4px;
    padding: 6px 8px;
    font-size: 15px;
    font-weight: 300;
}

QLineEdit:focus {
    /* Borde de enfoque con el azul claro primario */
    border: 1px solid #0078d7;
    background-color: #3D4A5B; /* Fondo sutil al enfocar */
}

/* === Labels === */
QLabel {
    /* Texto claro */
    color: #F0F0F0;
    font-weight: 300;
    font-size: 15px;
}

/* Label inferior o secundario */
#label {
    /* Texto gris claro para secundario */
    color: #A0A0A0;
    font-size: 13px;
    font-style: italic;
}

/* === Stacked Widget === */
QStackedWidget {
    background-color: #2D2D2D;
    border: 1px solid #444444;
    border-radius: 6px;
}

/* === Scrollbars === */
QScrollBar:vertical {
    border: none;
    background: #383838; /* Fondo oscuro para el scrollbar */
    width: 10px;
    border-radius: 4px;
}
QScrollBar::handle:vertical {
    background: #555555; /* Handle gris oscuro */
    min-height: 20px;
    border-radius: 4px;
}
QScrollBar::handle:vertical:hover {
    /* Handle hover azul claro */
    background: #0078d7;
}
QScrollBar::add-line:vertical,
QScrollBar::sub-line:vertical {
    height: 0;
}

/* === Layouts === */
QVBoxLayout, QHBoxLayout {
    spacing: 10px;
}

/* === Botones laterales (Botones de Navegación) === */
#boton_agregar, #boton_buscar, #boton_actualizar, #boton_eliminar, #boton_consultar {
    /* Fondo y borde para botones laterales */
    background-color: #383838; /* Fondo igual al panel lateral */
    border: 1px solid #555555;
    color: #F0F0F0;
    font-weight: 400;
    font-size: 15px;
}

#boton_agregar:hover, 
#boton_buscar:hover, 
#boton_actualizar:hover, 
#boton_eliminar:hover,
#boton_consultar:hover {
    /* Fondo de hover lateral azul claro y sutil */
    background-color: #0078d7; /* Usamos el azul primario */
    border: 1px solid #0078d7;
    color: #ffffff;
}

#boton_agregar:pressed, 
#boton_buscar:pressed, 
#boton_actualizar:pressed, 
#boton_eliminar:pressed ,
#boton_consultar:pressed{
    /* Color de pressed sutil */
    background-color: #005a9e;
}

/* === Tooltips === */
QToolTip {
    background-color: #383838;
    color: #F0F0F0;
    /* Borde con el azul claro primario */
    border: 1px solid #0078d7;
    padding: 6px;
    border-radius: 4px;
    font-size: 14px;
}</string>
